<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="seq_Department_order" model="ir.sequence">
            <field name="name">Department sequence</field>
            <field name="code">department.department</field>
            <field name="prefix">DP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

          <record id="seq_Employee_order" model="ir.sequence">
            <field name="name">Employee sequence</field>
            <field name="code">employee.employee</field>
            <field name="prefix">EM</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>